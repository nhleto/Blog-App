<h1>Articles Tagged with <%= @tag %></h1>
<ul>
  <% @tag.articles.each do |article| %>
    <li><%= link_to article.title, article_path(article) %>
  <% end %>
</ul>
<%= link_to 'delete tags', tag_path(@tag), method: :delete, data: {confirm: 'Delete tag?'} %>
<p><%= link_to '<<Back to Tag List', tags_path %></p>